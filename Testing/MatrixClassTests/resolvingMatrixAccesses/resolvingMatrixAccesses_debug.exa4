Domain global< [0.0, 0.0] to [0.1, 0.1] >



Stencil Centering{
	[1, 0] => 0.25
	[-1, 0] => 0.25
	[0, 1] => 0.25
	[0, -1] => 0.25
}

Layout mat_layout< Matrix<Real,2,2>, Node >@all {
	duplicateLayers = [0, 0]
	ghostLayers = [0, 0]
}

Layout mat_layout2< Matrix<Real,3,1>, Node >@all {
	duplicateLayers = [0, 0]
	ghostLayers = [0, 0]
}

Layout mat_layout3< Real, Node >@all {
