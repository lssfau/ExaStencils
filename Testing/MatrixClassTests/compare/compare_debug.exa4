Domain global< [0.0, 0.0] to [0.1, 0.1] >



Function Application {
	Var t : Matrix<Real,2,2> = { { 1.0, 2.0 }, { 4.0, 3.0 } }
	Var t_inverse : Matrix<Real,2,2> = inverse ( t )
	Var res : Matrix<Real,2,2> = { { -0.6, 0.4 }, { 0.8, -0.2 } }
	Var prec : Double = 1.0E-4
	compare ( t_inverse, res, prec )
}

