}

noinline Function f (in : Matrix<Double, 2, 2>)  : Matrix<Double, 2, 2> {
	Var i : Int = 0
	Var j : Int = 0		
	repeat 2 times count i {
		repeat 2 times count j {
			setElement(in,i,j,5)
		}
	}
	return in
}