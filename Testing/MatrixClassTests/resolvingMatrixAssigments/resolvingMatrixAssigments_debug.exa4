Domain global< [0.0, 0.0] to [0.1, 0.1] >



Function Application {
	Var prec : Double = 0.001
	Var m1 : Matrix<Real,2,2> = { { 1.0, 2.0 }, { 3.0, 4.0 } }
	Var m5 : Matrix<Real,2,2> = { { 1.0, 2.0 }, { 3.0, 4.0 } }
	m5 += m1
	m5 -= m1
	m5 *= m1
	m5 /= m1
}

